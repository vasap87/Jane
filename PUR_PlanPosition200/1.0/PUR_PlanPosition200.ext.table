      <column default-width="100" field="DOC.IND_WASPUBLISHER" name="Позиция была размещена в ЕИС" tooltip="Позиция была размещена в ЕИС"/>
      <column default-width="150" field="DOC.BI_EISPOSNUMBER" name="Порядковый номер позиции в ЕИС" tooltip="Порядковый номер позиции в ЕИС"/>
      <column default-width="250" field="DOC.TEI_EISPDATETZ" name="Дата и время размещения в ЕИС"/>
      <column default-hidden="true" default-width="88" field="DOC.CU_BRANCHCODE" name="Код филиала" tooltip="Код филиала"/>
      <column default-width="200" field="DOC.CU_BRANCHNAME" name="Структурное подразделение / Филиал" tooltip="Структурное подразделение / Филиал"/>
