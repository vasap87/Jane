      <column alignment="RIGHT" default-width="187" field="DOC.TOT_SUMNEXT" name="ФО на текущий финансовый год" tooltip="Сумма текущего года планового периода" type="DECIMAL" pattern="#,###,##0.00000"/>
      <column alignment="RIGHT" default-width="187" field="DOC.TOT_SUMFIRST" name="ФО на первый год планового периода" tooltip="Сумма первого года планового периода" type="DECIMAL" pattern="#,###,##0.00000"/>
      <column alignment="RIGHT" default-width="187" field="DOC.TOT_SUMSEC" name="ФО на второй год планового периода" tooltip="Сумма второго года планового периода" type="DECIMAL" pattern="#,###,##0.00000"/>
      <column alignment="RIGHT" default-width="187" field="DOC.TOT_SUMSUBSEC" name="ФО на последующие годы" tooltip="Сумма последующих лет планового периода" type="DECIMAL" pattern="#,###,##0.00000"/>
