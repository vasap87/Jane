      <column default-width="206" field="DOC.BI_EXPECTEDRESULT" name="Ожидаемый результат" tooltip="Ожидаемый результат"/>
      <column default-width="202" field="DOC.BI_DESCRIPTION" name="Описание закупки " tooltip="Описание закупки "/>
      <column default-width="225" field="DOC.BI_COMPLEXITY" name="Сведения о технической сложности закупки" tooltip="Специальный характер укрупненной закупки"/>
      <column default-width="100" field="DOC.BI_PUBLICDISCUSSION" name="Обязательное общественное обсуждение" tooltip="Обязательное общественное обсуждение"/>
      <column default-width="250" field="DOC.BI_TERMSTR" name="Срок осуществления закупок" tooltip="Срок осуществления закупок"/>
